#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tar.gz@1.0.7/node_modules/tar.gz/bin/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tar.gz@1.0.7/node_modules/tar.gz/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tar.gz@1.0.7/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tar.gz@1.0.7/node_modules/tar.gz/bin/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tar.gz@1.0.7/node_modules/tar.gz/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tar.gz@1.0.7/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/targz" "$@"
else
  exec node  "$basedir/../../bin/targz" "$@"
fi
