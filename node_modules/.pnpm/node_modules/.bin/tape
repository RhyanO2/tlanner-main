#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tape@4.17.0/node_modules/tape/bin/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tape@4.17.0/node_modules/tape/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tape@4.17.0/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tape@4.17.0/node_modules/tape/bin/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tape@4.17.0/node_modules/tape/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/tape@4.17.0/node_modules:/mnt/c/Users/Master/Projetos/pessoal/tlanner-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tape/bin/tape" "$@"
else
  exec node  "$basedir/../tape/bin/tape" "$@"
fi
